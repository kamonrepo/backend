<configuration>
	<system.webServer>
	
	     <validation validateIntegratedModeConfiguration="false" />

         <iisnode loggingEnabled="true" debuggingEnabled="true" />

		<handlers>
			<add name="iisnode" path="index.js" verb="*" modules="iisnode" />
		</handlers>
		
		<rewrite>
			<rules>
				<rule name="myapp">
					<match url="/*" />
					<action type="Rewrite" url="index.js" />
				</rule>
			</rules>
		</rewrite>

	</system.webServer>
</configuration>